{
  "name": "humaneight",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo & npm run codegen",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "reset": "rm -rf .next node_modules pnpm-lock.yaml ; pnpm i",
    "preview": "next build && next start",
    "codegen": "chokidar \"./graphql/**/*.gql\" -c \"pnpm codegen:website\" & chokidar \"./shopify/graphql/**/*.gql\" -c \"pnpm codegen:shopify\"",
    "codegen:website": "graphql-codegen --config graphql.config.ts --project default",
    "codegen:shopify": "graphql-codegen --config graphql.config.ts --project shopify"
  },
