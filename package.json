	"private": true,
	"scripts": {
		"dev": "next dev & npm run codegen",
		"barrel:watch": "watch 'node ./lib/scripts/barrel' ./components",
		"build": "npm run prebuild && next build",
		"prebuild": "tsx prebuild",
		"start": "next start",
		"lint": "next lint",
		"reset": "rm -rf .next node_modules pnpm-lock.yaml ; pnpm i",
		"preview": "next build && next start",
		"utils:link": "pnpm link /Users/bebejane/Projects/next-dato-utils",
		"utils:unlink": "pnpm unlink /Users/bebejane/Projects/next-dato-utils && pnpm up dato-next-utils",
		"codegen": "chokidar \"./graphql/**/*.gql\" -c \"pnpm codegen:website\" & chokidar \"./shopify/graphql/**/*.gql\" -c \"pnpm codegen:shopify\"",
		"codegen:website": "graphql-codegen --config graphql.config.ts --project default",
		"codegen:shopify": "graphql-codegen --config graphql.config.ts --project shopify"
	},
